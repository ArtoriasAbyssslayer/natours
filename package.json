{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "cross-env NODE_ENV=development ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --port 3000 --host 127.0.0.1 --dist-dir ./public/js/bundled --no-hmr",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled --no-hmr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtoriasAbyssslayer/NodeJS-Backend-Course.git"
  },
  "keywords": [
    "nodejs",
    "mongodb",
    "express"
  ],
  "author": "Harry Filis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArtoriasAbyssslayer/NodeJS-Backend-Course/issues"
  },
  "homepage": "https://github.com/ArtoriasAbyssslayer/NodeJS-Backend-Course#readme",
